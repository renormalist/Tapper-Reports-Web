<%args>
$precondition
</%args>

% use YAML::Syck;

<h1>Precondition ID <% $precondition->{id} %></h1>
<h4><a href="/artemis/preconditions/new/<% $precondition->{id} %>" title="Create a new precondition and use this precondition as a template.">Create similar Precondition</a></h4>
<h4><a href="/artemis/preconditions/change/<% $precondition->{id} %>" title="Change values of this preconditions without creating a new one">Change Precondition</a></h4>
<h4><a href="/artemis/preconditions/delete/<% $precondition->{id} %>" title="Information about testruns using this precondition will be wrong.">Delete Precondition</a></h4>

% delete $precondition->{id};
<pre>
<% Dump $precondition %>
</pre>




