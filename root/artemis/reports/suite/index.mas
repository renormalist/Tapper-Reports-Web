<%args>
        $filter_suite
        @this_weeks_reportlists
</%args>

% if ($filter_suite) {

        <h3>This weeks reports by suite: <span><% $filter_suite->name %></span></h3>
        <& /artemis/reports/week_reportlists.mas, week_reportlists => \@this_weeks_reportlists &>

% } else {

        <h3>This weeks reports by suite: <span>unknown</span></h3>
        No info available
% }

