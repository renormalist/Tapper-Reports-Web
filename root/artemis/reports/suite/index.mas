<%args>
        $filter_suite
        $reportlist
</%args>

% if ($filter_suite) {

        <h3>All reports by suite: <span><% $filter_suite->name %></span></h3>
        <& /artemis/reports/all_reportlist.mas, reportlist => $reportlist &>

% } else {

        <h3>All reports by suite: <span>unknown</span></h3>
        No info available

% }

